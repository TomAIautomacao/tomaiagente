generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum InstanceConnectionStatus {
  open
  close
  connecting
}

enum DeviceMessage {
  ios
  android
  waweb
  unknown
  desktop
}

enum SessionStatus {
  connected
  disconnected
}

enum TriggerType {
  all
  keyword
  advanced
}

enum TriggerOperator {
  contains
  equals
  startsWith
  endsWith
}
